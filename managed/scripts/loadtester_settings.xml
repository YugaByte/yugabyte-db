  <settings>
    <servers>
      <server>
        <id>yugabyte-snapshots</id>
        <username></username>
        <password></password>
      </server>
      <server>
        <id>yugabyte-releases</id>
        <username></username>
        <password></password>
      </server>
    </servers>

   <profiles>
    <profile>
      <id>nexus</id>
      <repositories>
        <repository>
          <id>yugabyte-snapshots</id>
          <url>http://10.150.0.12/repository/yugabyte-snapshots/</url>
          <releases><enabled>false</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </repository>
        <repository>
          <id>yugabyte-releases</id>
          <url>http://10.150.0.12/repository/yugabyte-releases/</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>false</enabled></snapshots>
        </repository>
      </repositories>
      <properties>
        <yugabyte.snapshots.repository.id>yugabyte-snapshots</yugabyte.snapshots.repository.id>
        <yugabyte.snapshots.repository.url>http://10.150.0.12/repository/yugabyte-snapshots/</yugabyte.snapshots.repository.url>
        <yugabyte.releases.repository.id>yugabyte-releases</yugabyte.releases.repository.id>
        <yugabyte.releases.repository.url>http://10.150.0.12/repository/yugabyte-releases/</yugabyte.releases.repository.url>
      </properties>
    </profile>
    <profile>
      <id>s3</id>
      <properties>
        <yugabyte.releases.repository.id>s3.release</yugabyte.releases.repository.id>
        <yugabyte.releases.repository.url>s3://releases.yugabyte.com</yugabyte.releases.repository.url>
        <yugabyte.snapshots.repository.id>s3.snapshot</yugabyte.snapshots.repository.id>
        <yugabyte.snapshots.repository.url>s3://snapshots.yugabyte.com</yugabyte.snapshots.repository.url>
      </properties>
    </profile>
   </profiles>

   <activeProfiles>
     <activeProfile>nexus</activeProfile>
   </activeProfiles>

  </settings>
